{
    "routes": [
        {
            "route": "/login",
            "rewrite": "/.auth/login/google"
        },
        {
            "route": "/logout",
            "rewrite": "/.auth/logout?post_logout_redirect_uri=/"
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/static/*"
        ]
    }
}